{
  "$schema": "http://json-schema.org/schema",
  "id": "http://xowl.org/platform/services/importation/schema",
  "definitions": {
    "Importer": {
      "type": "object",
      "properties": {
        "type": {
          "enum": [
            "org.xowl.platform.services.importation.Importer"
          ]
        },
        "identifier": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "wizardUri": {
          "type": "string"
        }
      },
      "required": [
        "type",
        "identifier",
        "name",
        "wizardUri"
      ]
    },
    "ImporterConfiguration": {
      "type": "object",
      "properties": {
        "type": {
          "enum": [
            "org.xowl.platform.services.importation.ImporterConfiguration"
          ]
        },
        "family": {
          "type": "string"
        },
        "superseded": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "version": {
          "type": "string"
        },
        "archetype": {
          "type": "string"
        }
      },
      "required": [
        "type"
      ]
    },
    "Importers": {
      "type": "array",
      "items": {
        "type": "object",
        "oneOf": [
          {
            "$ref": "#/definitions/Importer"
          }
        ]
      }
    },
    "Document": {
      "type": "object",
      "properties": {
        "type": {
          "enum": [
            "org.xowl.platform.services.importation.Document"
          ]
        },
        "identifier": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "uploadDate": {
          "type": "string"
        },
        "uploader": {
          "type": "string"
        },
        "fileName": {
          "type": "string"
        }
      },
      "required": [
        "type",
        "identifier",
        "name",
        "uploadDate",
        "uploader",
        "fileName"
      ]
    },
    "Documents": {
      "type": "array",
      "items": {
        "type": "object",
        "oneOf": [
          {
            "$ref": "#/definitions/Document"
          }
        ]
      }
    }
  }
}